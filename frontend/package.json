{
  "name": "geo-hearo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "format": "prettier --write .",
    "minify": "find public/data -type f \\( -name '*.json' -o -name '*.geojson' \\) -exec sh -c 'jq -c . \"$1\" > \"$1.tmp\" && mv \"$1.tmp\" \"$1\"' _ {} \\;"
  },
  "dependencies": {
    "@plausible-analytics/tracker": "^0.4.4",
    "@tailwindcss/vite": "^4.1.18",
    "gsap": "^3.14.2",
    "lucide-vue-next": "^0.562.0",
    "maplibre-gl": "^5.15.0",
    "tailwindcss": "^4.1.18",
    "vant": "^4.9.22",
    "vue": "^3.5.24",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "prettier": "^3.7.4",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vitest": "^4.0.17",
    "vue-tsc": "^3.1.4"
  }
}
